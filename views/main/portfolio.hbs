{{>header}}

<div id="portfolio" >
    {{#each projects}}
    <div class="row p-5" style="border:1px solid gray;">
        <div class="col-3">
            <img class="img-fluid" src="{{this.img}}">
        </div>
        <div class="col-6 px-5">
            <h2>{{this.name}}</h2>
            <p class="text-muted">
                {{this.desc}}
            </p>
        </div>
        <div class="col-3">
            <div>
                <div class="row">
                    <div class="col-6 text-right">Online sinds -  </div>
                    <div class="col-6">{{this.date}}</div>
                </div>
                <div class="row">
                    <div class="col-6 text-right">Focus - </div>
                    <div class="col-6">{{this.focus}}</div>
                </div>
                <div class="row">
                    <div class="col-6 text-right">Technologien - </div>
                    <div class="col-6"><small>{{this.tech}}</small></div>
                </div>
                <div class="row">
                    <div class="col-6 text-right">Link - </div>
                    <div class="col-6"><a about="blank" href="{{this.link}}">Naar website &rarr;</a></div>
                </div>
            </div>
        </div>
    </div>
    {{/each}}
    
    
